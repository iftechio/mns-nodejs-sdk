{
  "name": "mns-node-sdk",
  "version": "1.0.0",
  "description": "unofficial sdk for Aliyun mns",
  "scripts": {
    "compile": "tsc",
    "clean": "rm -rf dist",
    "purge": "rm -rf package-lock.json node_modules && npm run clean",
    "test": "rm -rf dist && npm run lint && tsc && ava -T 10000 dist/tests/*.test.js",
    "lint": "tslint --project .",
    "prettier": "prettier --config .prettierrc --write '{lib,tests,examples}/**/*.ts' index.ts",
    "prepublishOnly": "npm run compile"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "request-promise-native": "^1.0.7",
    "xml2js": "^0.4.19",
    "xml-escape": "1.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.137",
    "@types/request-promise-native": "^1.0.15",
    "@types/xml2js": "0.4.4",
    "@types/xml-escape": "1.1.0",
    "ava": "2.3.0",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-jike-node": "0.0.17",
    "typescript": "^3.6.3"
  },
  "author": "liukaixiang<liukaixiangid@gmail.com>",
  "license": "MIT",
  "keywords": [
    "MNS",
    "Message",
    "Service",
    "Aliyun",
    "Alicloud"
  ]
}
